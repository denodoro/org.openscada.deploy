<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="common"
    nsURI="http://openscada.org/Configurator/Modules/Common" nsPrefix="common">
  <eSubpackages name="network" nsURI="http://openscada.org/Configurator/Modules/Common/Network"
      nsPrefix="network">
    <eClassifiers xsi:type="ecore:EClass" name="NetworkModule" eSuperTypes="platform:/plugin/org.openscada.configurator.model/model/configurator.ecore#//Module">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="networkFile" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          defaultValueLiteral=""/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="prefix" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="overrideGeneratedFile"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Override the absolute name of the generated IO list file. The default is &quot;IOList-generated-network.xls&quot; in the projects generated directory."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="scripts" nsURI="http://openscada.org/Configurator/Modules/Common/Scripts"
      nsPrefix="scripts">
    <eClassifiers xsi:type="ecore:EClass" name="ScriptsModule" eSuperTypes="platform:/plugin/org.openscada.configurator.model/model/configurator.ecore#//Module">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="scriptsFile" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LegacyFormulaModule" eSuperTypes="platform:/plugin/org.openscada.configurator.model/model/configurator.ecore#//Module">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="formulaFile" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="processing" nsURI="http://openscada.org/Configurator/Modules/Common/Processing"
      nsPrefix="processing">
    <eClassifiers xsi:type="ecore:EClass" name="ImportModule" eSuperTypes="platform:/plugin/org.openscada.configurator.model/model/configurator.ecore#//Module">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="path" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
